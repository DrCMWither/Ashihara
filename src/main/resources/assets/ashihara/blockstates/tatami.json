{
  "multipart": [
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "ashihara:block/tatami/tatami"
      }
    },
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "ashihara:block/tatami/tatami",
        "y": 270
      }
    },
    {
      "when": {
        "axis": "x",
        "left": false
      },
      "apply": {
        "model": "ashihara:block/tatami/edges/n"
      }
    },
    {
      "when": {
        "axis": "x",
        "right": false
      },
      "apply": {
        "model": "ashihara:block/tatami/edges/s"
      }
    },
    {
      "when": {
        "axis": "z",
        "left": false
      },
      "apply": {
        "model": "ashihara:block/tatami/edges/e"
      }
    },
    {
      "when": {
        "axis": "z",
        "right": false
      },
      "apply": {
        "model": "ashihara:block/tatami/edges/w"
      }
    },
    {
      "when": {
        "xcut": true
      },
      "apply": {
        "model": "ashihara:block/tatami/edges/xcut"
      }
    },
    {
      "when": {
        "zcut": true
      },
      "apply": {
        "model": "ashihara:block/tatami/edges/zcut"
      }
    }
  ]
}