{
  "multipart": [
    {
      "when": {
        "half": "lower"
      },
      "apply": {
        "model": "ashihara:block/lamp_base"
      }
    },
    {
      "when": {
        "facing": "north",
        "half": "upper"
      },
      "apply": {
        "model": "ashihara:block/oil_plate_stick"
      }
    },
    {
      "when": {
        "facing": "east",
        "half": "upper"
      },
      "apply": {
        "model": "ashihara:block/oil_plate_stick",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south",
        "half": "upper"
      },
      "apply": {
        "model": "ashihara:block/oil_plate_stick",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west",
        "half": "upper"
      },
      "apply": {
        "model": "ashihara:block/oil_plate_stick",
        "y": 270
      }
    }
  ]
}