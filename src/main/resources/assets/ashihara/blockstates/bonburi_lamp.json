{
  "multipart": [
    {
      "when": {
        "half": "lower"
      },
      "apply": {
        "model": "ashihara:block/lamp_base"
      }
    },
    {
      "when": {
        "axis": "x",
        "half": "upper"
      },
      "apply": {
        "model": "ashihara:block/bonburi_lamp",
        "y": 90
      }
    },
    {
      "when": {
        "axis": "z",
        "half": "upper"
      },
      "apply": {
        "model": "ashihara:block/bonburi_lamp"
      }
    }
  ]
}